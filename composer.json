{
  "name": "andreaskastl/openweatherapi",
  "type": "typo3-cms-extension",
  "description": "Displays weather forecast for a selected location in frontend based on wetter.com openweather API",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": "^8",
    "typo3/cms-core": "^12.4 || ^13.4"
  },
  "authors": [
    {
      "name": "Andreas Kastl",
      "role": "Developer",
      "email": "typo3@andreaskastl.de",
      "homepage": "https://github.com/andreaskastl/"
    }
  ],
  "keywords": [
    "typo3",
    "weather",
    "foreacast",
    "openweatherapi"
  ],
  "support": {
    "issues": "https://github.com/andreaskastl/openweatherapi/issues"
  },   
  "replace": {
    "typo3-ter/openweatherapi": "self.version"
  },
  "autoload": {
    "psr-4": {
      "AndreasKastl\\Openweatherapi\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "openweatherapi"
    }
  }
}